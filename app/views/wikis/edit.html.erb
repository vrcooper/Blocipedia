<h1>Edit Wiki</h1>

<%= render partial: 'form', locals: {wiki: @wiki } %>

<% if current_user.role == 'premium' || 'admin' %>
  
<h3>Manage Collaborators</h3>
  
<div class="row">
  <div class="col-md-8">
    <%= link_to "Add Collaborator", wiki_collaborators_path(@wiki, @users,:id), method: :wiki, class: 'btn btn-success' %>
